{
  "name": "9GAG Repost",
  "version": "1.0.2",
  "description": "Uses FB comments to generate repost values for a 9GAG Post",
  "update_url": "http://home.iitb.ac.in/~mayank.singhal/chrome/9gag_repost/updates.xml",
  "permissions": [
    "tabs",
    "http://9gag.com/*"
  ],
  "icons": {
    "48" : "icon.png",
    "128" : "thumbnail.png"
  },
  /* "browser_action": {
    "default_icon": "icon.png"
  },*/
  //"background_page" : "background.html",
  "content_scripts": [
    {
      "matches": ["http://9gag.com/*"],
      "js" : ["js/jquery.min.js", "js/contentscript.js"],
      "css" : ["css/main.css"]
    }
  ]
}
